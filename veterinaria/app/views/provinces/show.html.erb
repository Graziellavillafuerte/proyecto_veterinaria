<%- model_class = Province -%>
<div class="row"></div>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nombre:</strong></dt>
  <dd><%= @province.name %></dd>
  <dt><strong>Id Departamento:</strong></dt>
  <dd><%= @province.department_id %></dd>
</dl>

<%= link_to t('.back', :default => t("CERRAR")),
              provinces_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("EDITAR")),
              edit_province_path(@province), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("ELIMINAR")),
              province_path(@province),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => '¿Está seguro de elliminar?')) },
              :class => 'btn btn-danger' %>
