<%- model_class = District -%>
<div class="row"></div>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nombre:</strong></dt>
  <dd><%= @district.name %></dd>
  <dt><strong>Id Provincia:</strong></dt>
  <dd><%= @district.province_id %></dd>
</dl>

<%= link_to t('.back', :default => t("CERRAR")),
              districts_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("EDITAR")),
              edit_district_path(@district), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("ELIMINAR")),
              district_path(@district),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
